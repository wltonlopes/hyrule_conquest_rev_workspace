<?xml version="1.0" encoding="utf-8"?>
<particles>

    <texture>art/textures/particles/dust_256a.png</texture>
    <blend mode="over"/>

    <constant name="emissionrate" value="35.0"/>

     <uniform name="lifetime" min="10.0" max="17.0"/>

    <uniform name="position.x" min="-15.0" max="15.0"/>
    <uniform name="position.z" min="-15.0" max="15.0"/>
    <constant name="position.y" value="1.0"/>

    <uniform name="angle" min="-3.14" max="3.14"/>

    <uniform name="velocity.x" min="-1.5" max="1.0"/>
    <uniform name="velocity.y" min="2.0" max="5.5"/>
    <uniform name="velocity.z" min="-1.5" max="1.0"/>
    <uniform name="velocity.angle" min="-2.0" max="3.0"/>

    <uniform name="size" min="5.0" max="10.5"/>

    <uniform name="color.r" min="0.7" max="0.8"/>
    <uniform name="color.g" min="0.7" max="0.7"/>
    <uniform name="color.b" min="0.5" max="0.6"/>

    <force y="-0.5"/>

</particles>

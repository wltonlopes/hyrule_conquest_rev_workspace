<?xml version="1.0" encoding="utf-8"?>
<particles>

    <texture>art/textures/particles/dust_256a.png</texture>
    <blend mode="over"/>
	
    <constant name="emissionrate" value="150.0"/>
    <uniform name="lifetime" min="0.9" max="5.0"/>

    <uniform name="position.x" min="-11.0" max="11.0"/>
    <uniform name="position.z" min="-11.0" max="11.0"/>
    <constant name="position.y" value="0.0"/>

    <uniform name="angle" min="-3.14" max="3.14"/>

    <uniform name="velocity.x" min="-7.1" max="7.1"/>
	<uniform name="velocity.y" min="0" max="7.1"/>
    <uniform name="velocity.z" min="-7.1" max="7.1"/>
    <uniform name="velocity.angle" min="-1.5" max="1.5"/>

    <uniform name="size" min="3.0" max="14.0"/>

    <force y="-0.1"/>

</particles>
